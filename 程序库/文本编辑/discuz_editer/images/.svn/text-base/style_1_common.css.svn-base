.ad_footerbanner { text-align: center; clear: both; margin: 5px }

/* ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ Side list ~~~~ */
