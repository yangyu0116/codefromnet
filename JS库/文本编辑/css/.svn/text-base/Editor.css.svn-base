	WIDTH: 20px
}
.BtnMouseOverUp
{
	BACKGROUND-COLOR: #98B5E2;
	BORDER: #000080	1px solid;
	HEIGHT: 21px;
	POSITION: absolute;
	TOP: 1px;
	WIDTH: 21px
}
.BtnMouseOverDown
{
	BACKGROUND-COLOR: #D7F1FE;
	BORDER: #000080 1px solid;
	HEIGHT: 21px;
	POSITION: absolute;
	TOP: 1px;
	WIDTH: 21px
}
.BtnDown
{
	BACKGROUND-COLOR: #98B5E2;
	BORDER: #000080 1px solid;
	HEIGHT: 21px;
